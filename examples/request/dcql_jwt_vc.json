{
  "credentials": [
    {
      "id": "example_jwt_vc",
      "format": "jwt_vp_json",
      "meta": {
        "proof_type_values": ["JsonWebSignature2020"]
      },
      "claims": [
        {
          "path": ["type"],
          "contains": ["IDCredential"]
        },
        {"path": ["credentialSubject", "family_name"]},
        {"path": ["credentialSubject", "given_name"]}
      ]
    }
  ]
}